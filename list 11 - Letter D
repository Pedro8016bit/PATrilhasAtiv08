using System;
using System.Linq;
using System.Collections.Generic;

namespace PATrilhasAtiv08
{
	class Program
	{
		static int maior(List <int> num)
		{
			return num.Max();
		}
		public static void Main(string[] args)
		{
			List <int> num = new List<int>();
			
			int quant = 0;
			
			
			Console.WriteLine("digite numeros ");
			quant = int.Parse(Console.ReadLine());
			
			
			for (int i =0; i<quant; i++)
			{
				Console.WriteLine("digite para a lista ");
				int nume = int.Parse(Console.ReadLine());
				num.Add(nume);
				
			}
			
			Console.WriteLine("o maior numero Ã© " + maior(num));
				
			Console.Write("Press any key to continue . . . ");
			Console.ReadKey(true);
		}
	}
}
